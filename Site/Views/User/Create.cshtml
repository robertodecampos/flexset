@{
    ViewData["Title"] = "Cadastrar Usuário";
}

<h2>@ViewData["Title"]</h2>

<br />

<form asp-controller="User" asp-action="Create" method="post" role="form">

    @Html.PartialAsync("_PersonalCreatePartial").Result

    <div style="float: right" class="form-group">
        <!-- Button -->
        <div class="col-sm-12 controls">
            <button id="btnLogin" type="submit" class="btn btn-success">
                Cadastrar
            </button>
        </div>
    </div>
</form>

@section Scripts {
    @await Html.PartialAsync("_ValidationScriptsPartial")
}